<facebinding>
	<FaceUnitSpec type="faceLexeme">
		<constraints>
			<constraint name="lexeme" value="REPEATED_BLINK"/>
		</constraints>
		<parametermap>
			<parameter src="amount" dst="intensity"/>
			<parameter src="http://asap-project.org/convanim:repetition" dst="repeats"/>
		</parametermap>
		<parameterdefaults>
			<parameterdefault name="targetname" value="Body_NG-mesh-morpher-yeux_NG01-1"/>			
			<parameterdefault name="intensity" value="1"/>
			<parameterdefault name="repeats" value="2"/>
		</parameterdefaults>
		<FaceUnit type="class" class="asap.realizerdemo.newanimations.SinusWaveFU"/>
	</FaceUnitSpec>	

  	<FaceUnitSpec type="faceFacs" namespace="http://www.bml-initiative.org/bml/coreextensions-1.0">
		<constraints>
		</constraints>
		<parametermap>
			<parameter src="amount" dst="intensity"/>
			<parameter src="au" dst="au"/>
			<parameter src="side" dst="side"/>
		</parametermap>
		<FaceUnit type="AU"/>
	</FaceUnitSpec>
	<FaceUnitSpec type="faceLexeme">
		<constraints>
			<constraint name="lexeme" value="RAISE_LEFT_BROW"/>
